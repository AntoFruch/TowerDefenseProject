    border-left-color: rgb(0, 255, 252);
    border-right-color: rgb(0, 255, 252);
    border-top-color: rgb(0, 255, 252);
    border-bottom-color: rgb(0, 255, 252);
}

.btn-map-label {
    font-size: 25px;
}

.start-label {
}

